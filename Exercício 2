#include<stdio.h>
int main (){
 float n1, n2, n3, media;
 printf ("informe a primeira nota:");
 scanf ("%f", &n1);
 printf ("informe a segunda nota:");
 scanf ("%f", &n2);
 printf ("informe a terceira nota:");
 scanf ("%f", &n3);
 media = (n1 * 2 + n2 * 3 + n3 * 5) / 10;
 printf ("\na media e: %.2f", media);
 return 0;
}
