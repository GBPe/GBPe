#include <stdio.h>
int main(){
float a,b,c;
printf("informe o valor de a:");
scanf("%d",&a);
printf("informe o valor de b:");
scanf("%d",&b);
printf("informe o valor de c:");
scanf("%d",&c);
 if(a + b > c && a + c > b && b + c > a){
        printf("e um triangulo ");
        if(a == b && a == c)
            printf("Equilatero\n");
        else
            if(a == b || a == c || b == c)
                printf("Isosceles\n");
            else
                printf("Escaleno\n");
    }
    else
        printf("nao e um triangulo");

return 0;
}
